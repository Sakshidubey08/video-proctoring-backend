{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/events/(.*)",
      "dest": "/api/events.js"
    },
    {
      "src": "/upload-video/(.*)",
      "dest": "/api/upload-video.js"
    },
    {
      "src": "/videos/(.*)",
      "dest": "/api/videos.js"
    }
  ]
}